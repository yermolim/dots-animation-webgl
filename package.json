{
  "name": "dots-animation-webgl",
  "version": "0.0.1",
  "description": "simple module for filling your html container with fancy responsive dots animation",
  "main": "dist/index.umd.min.js",
  "module": "dist/index.esm.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "buildts": "tsc",
    "buildru": "rollup -c",
    "build": "tsc && rollup -c && dts-bundle-generator -o dist/index.d.ts src/dots-animation.ts",
    "start": "npm run build && copyfiles -f ./dist/index.umd.min.js ./demo && lite-server -c ls-config.json",
    "ppublish": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yermolim/dots-animation-webgl.git"
  },
  "keywords": [
    "web",
    "front-end",
    "responsive",
    "typescript",
    "animation",
    "webgl",
    "background",
    "dots",
    "customization"
  ],
  "author": "Volodymyr Yermolenko <yermolim@gmail.com> (https://github.com/yermolim)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yermolim/dots-animation-webgl/issues"
  },
  "homepage": "https://github.com/yermolim/dots-animation-webgl#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "copyfiles": "^2.4.0",
    "dts-bundle-generator": "^5.4.0",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "lite-server": "^2.5.4",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
